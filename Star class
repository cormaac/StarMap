class Star
{


void Table()
{
  Table stars;
  
  stars = loadTable("starmap.csv", "header");
   
    println(stars.getRowCount() + " total rows in table"); 

  
   for (TableRow row : stars.rows()) {
    
    int hab = row.getInt("Hab?");
    String DisplayName = row.getString("Display Name");
    int Distance = row.getInt("Distance");
    int AbsMag = row.getInt("AbsMag");  
    
    println("habitable? " + hab + "Name is " + DisplayName + "Distance is "+ Distance + "Star Size is " + AbsMag);
    
  
}
  }
}
