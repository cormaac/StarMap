//Cormac Montague C12701485

Star b;

void setup()
{
  size(800,800);
  b = new Star();
  background(0);
}



void draw()
{
  b.Table();
  int x=0;
  int y=0;
  
  for(x=0; x<width; x=x+800/8)
  {
    line(x,0,x,height);
    stroke(228,21,235);
  }
  
  for(y=0; y<height; y=y+800/8)
  {
    line(0,y,width,y);
    stroke(228,21,235);
  }
}
